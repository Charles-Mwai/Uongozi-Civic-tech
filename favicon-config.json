{
  "masterPicture": "./public/logo.jpg",
  "iconsPath": "/",
  "design": {
    "ios": {
      "pictureAspect": "backgroundAndMargin",
      "backgroundColor": "#ffffff",
      "margin": "14%"
    },
    "desktopBrowser": {},
    "windows": {
      "pictureAspect": "noChange",
      "backgroundColor": "#ffffff",
      "onConflict": "override"
    },
    "androidChrome": {
      "pictureAspect": "noChange",
      "themeColor": "#ffffff",
      "manifest": {
        "name": "Uongozi Civic Tech",
        "display": "standalone",
        "orientation": "notSet",
        "onConflict": "override"
      }
    },
    "safariPinnedTab": {
      "pictureAspect": "silhouette",
      "themeColor": "#5bbad5"
    }
  },
  "settings": {
    "compression": 5,
    "scalingAlgorithm": "Mitchell",
    "errorOnImageTooSmall": false
  }
}
